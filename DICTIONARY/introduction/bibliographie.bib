@article{lin2002,
	Abstract = {This paper reanalyzes the basic orientation system of rGyalrong, a
Tibeto-Burman language of northwestern Sichuan. The fundamental solar
orientation axis \{east ↔ west\}, overlooked in previous analyses, is vindicated
